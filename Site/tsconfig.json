{
  "compilerOptions": {
    "baseUrl": ".",
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "jsx": "react",
    "sourceMap": true,
    "skipDefaultLibCheck": true,
    "strict": true,
    "types": [
      "webpack-env",
      "aspnet-webpack-react"
    ],
    "lib": [ "dom", "es5", "es2015", "es2015.symbol" "es2015.symbol.wellknown" ]
  },
  "exclude": [
    "bin",
    "node_modules"
  ]
}
